          MODULE=libXcursor
         VERSION=1.2.1
          SOURCE=$MODULE-$VERSION.tar.xz
      SOURCE_URL=$XORG_URL/individual/lib/
      SOURCE_VFY=sha256:46c143731610bafd2070159a844571b287ac26192537d047a39df06155492104
   MODULE_PREFIX=${X11R7_PREFIX:-/usr}
        WEB_SITE=http://www.x.org
         ENTERED=20060120
         UPDATED=20220616
           SHORT="The X.Org cursor management library"

cat << EOF
libXcursor is a library designed to help locate and load X cursors from within
the X Window System. Cursors can be loaded from files or memory and can exist
in several sizes.
EOF
