          MODULE=xmessage
         VERSION=1.0.6
          SOURCE=$MODULE-$VERSION.tar.xz
      SOURCE_URL=$XORG_URL/individual/app/
      SOURCE_VFY=sha256:d2eac545f137156b960877e052fcc8e29795ed735c02f7690fd7b439e6846a12
   MODULE_PREFIX=${X11R7_PREFIX:-/usr}
        WEB_SITE=http://www.x.org
         ENTERED=20060130
         UPDATED=20220809
           SHORT="The X.Org xmessage application"

cat << EOF
xmessage displays a window containing a message from the command line, a file,
or standard input.
The program is typically used by shell scripts to display information to the
user or ask the user to make a choice.
EOF
