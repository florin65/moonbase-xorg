          MODULE=xkeyboard-config
         VERSION=2.35.1
          SOURCE=$MODULE-$VERSION.tar.xz
      SOURCE_URL=$XORG_URL/individual/data/$MODULE
      SOURCE_VFY=sha256:18ce50ff0c74ae6093062bce1aeab3d363913ea35162fe271f8a0ce399de85cc
   MODULE_PREFIX=${X11R7_PREFIX:-/usr}
        WEB_SITE=https://freedesktop.org/wiki/Software/XKeyboardConfig
         ENTERED=20070329
         UPDATED=20220210
           SHORT="keyboard data files for the X.Org server"
       LDD_CHECK=off

cat << EOF
The non-arch keyboard configuration database for X Window. The goal is to
provide the consistent, well-structured, frequently released open source of
X keyboard configuration data for X Window System implementations (free, open
source and commercial). The project is targeted to XKB-based systems.
EOF
